{
  "redirects": [
    {
      "source": "/",
      "destination": "/login",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://server-kelolasaji-production.up.railway.app/api/:path"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
